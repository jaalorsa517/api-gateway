{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "api-gateway",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    },
  "include": [
    "function/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "api-gateway",
    "test"
  ]
}